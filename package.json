    "unassert": "^1.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-mourner": "^3.0.0",
    "rollup": "^2.77.2",
    "tape": "^5.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
