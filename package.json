  "types": "./dist/src/unassert.d.ts",
  "main": "./dist/unassert-rollup-plugin.js",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "acorn": "^8.8.0",
